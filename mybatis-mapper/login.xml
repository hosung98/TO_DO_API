<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="testMapper">  
  <select id="testBasic">
    SELECT
      *
    FROM
      T_IFO_MEMBERS 
    WHERE
      id = #{id}
      AND password = #{password}
  </select>
</mapper>